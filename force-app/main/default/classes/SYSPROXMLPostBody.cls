    public static String populateDynamicXMLIn(List<OrderItem> orderList, String orderId){
        DOM.Document doc = new DOM.Document();
        dom.XmlNode root = doc.createRootElement('SalesOrders', 'http://www.w3.org/2001/XMLSchema-instance', '');
        dom.XmlNode orders = root.addChildElement('Orders', null, null);
        Order order = [SELECT Id, Purchase_Order_Number__c, Account.Syspro_Id__c, EffectiveDate, ReqShipDate__c, Retail_Store__r.Shipping_Instructions_Code__c, Type FROM Order WHERE Id =: orderId];
        dom.XmlNode orderHeader = orders.addChildElement('OrderHeader', null, null);
        orderHeader.addChildElement('CustomerPoNumber', null, null).addTextNode(''+order.Purchase_Order_Number__c);
        orderHeader.addChildElement('OrderActionType', null, null).addTextNode('A');
        orderHeader.addChildElement('Customer', null, null).addTextNode(''+order.Account.Syspro_Id__c);
        orderHeader.addChildElement('OrderDate', null, null).addTextNode(''+order.EffectiveDate);
        orderHeader.addChildElement('Email', null, null).addTextNode('');
        orderHeader.addChildElement('RequestedShipDate', null, null).addTextNode(''+order.ReqShipDate__c);
        if(order.Type == 'Credit'){
        	orderHeader.addChildElement('OrderType', null, null).addTextNode('4');
        }
        if(order.Retail_Store__r.Shipping_Instructions_Code__c!=null)
        {
            if(order.Retail_Store__r.Shipping_Instructions_Code__c.isNumeric() && Integer.valueOf(order.Retail_Store__r.Shipping_Instructions_Code__c) < 10){
                    orderHeader.addChildElement('MultiShipCode', null, null).addTextNode('0'+Integer.valueOf(order.Retail_Store__r.Shipping_Instructions_Code__c.trim()));
            }
            else orderHeader.addChildElement('MultiShipCode', null, null).addTextNode(''+order.Retail_Store__r.Shipping_Instructions_Code__c.trim());
        }
         
        dom.XmlNode orderDetails = orders.addChildElement('OrderDetails', null, null);
        
        //for each order product line item, add a stock line XmlNode
        for(OrderItem current: orderList){
            dom.XmlNode stockline = orderDetails.addChildElement('StockLine', null, null);
            stockline.addChildElement('CustomerPoLine', null, null).addTextNode('1');
            stockline.addChildElement('LineActionType', null, null).addTextNode('A');
            stockline.addChildElement('StockCode', null, null).addTextNode(''+current.Product2.StockKeepingUnit);
            stockline.addChildElement('Warehouse', null, null).addTextNode('FQ'); //hard coded
            stockline.addChildElement('OrderQty', null, null).addTextNode(''+Integer.valueOf(current.Quantity));
            stockline.addChildElement('OrderUom', null, null).addTextNode('EA'); //hard coded
            stockline.addChildElement('Price', null, null).addTextNode(''+Double.valueOf(current.UnitPrice));
            stockline.addChildElement('PriceUom', null, null).addTextNode('EA'); //hard coded
        }
        String xmlstring = doc.toXmlString();
        return xmlstring;
    }
    
